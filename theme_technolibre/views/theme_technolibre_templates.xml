<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <template id="theme_technolibre_asset" inherit_id="website.assets_frontend" name="TechnoLibre theme website" active="True">
    <xpath expr="//link[last()]" position="after">
      <link rel="stylesheet" type="text/scss" href="/theme_technolibre/static/src/scss/_variables.scss"/>
      <link rel="stylesheet" type="text/scss" href="/theme_technolibre/static/src/scss/button.scss"/>
      <link rel="stylesheet" type="text/scss" href="/theme_technolibre/static/src/scss/custom.scss"/>
      <link rel="stylesheet" type="text/scss" href="/theme_technolibre/static/src/scss/font.scss"/>
      <link rel="stylesheet" type="text/scss" href="/theme_technolibre/static/src/scss/footer.scss"/>
      <link rel="stylesheet" type="text/scss" href="/theme_technolibre/static/src/scss/navbar.scss"/>
      <link rel="stylesheet" type="text/scss" href="/theme_technolibre/static/src/scss/form.scss"/>
      <link rel="stylesheet" type="text/scss" href="/theme_technolibre/static/src/scss/hero.scss"/>
    </xpath>
  </template>

  <template id="theme_technolibre_var" inherit_id="website._assets_primary_variables">
    <xpath expr="//link[@href='/website/static/src/scss/primary_variables.scss']" position="replace">
      <link rel="stylesheet" type="text/scss" href="/theme_technolibre/static/src/scss/primary_variables.scss"/>
    </xpath>
  </template>
</odoo>
