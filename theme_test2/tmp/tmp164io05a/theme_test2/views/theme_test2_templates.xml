<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="theme_test2_assets_frontend" inherit_id="website.assets_frontend" name="test_theme" active="True">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/theme_test2/static/src/scss/_variables.scss" />
            <link rel="stylesheet" type="text/scss" href="/theme_test2/static/src/scss/custom.scss" />
        </xpath>
    </template>
    <template id="theme_test2_assets_primary_variables" inherit_id="website._assets_primary_variables">
        <xpath expr="//link[@href='/website/static/src/scss/primary_variables.scss']" position="replace">
            <link rel="stylesheet" type="text/scss" href="/theme_test2/static/src/scss/primary_variables.scss" />
        </xpath>
    </template>
</odoo>
